{
    "name": "DevAnsible",
    "dockerFile": "Dockerfile",
    "customizations": {
        "vscode": {
            "settings": {
                "cSpell.language": "en,es",
                "files.associations": {
                    "*plays.yml": "ansible",
                    "*init.yml": "yaml"
                },
                "ansible.python.interpreterPath": "/bin/python3"
            },
            "extensions": [
                "redhat.vscode-yaml",
                "shardulm94.trailing-spaces",
                "redhat.ansible"
            ]
        }
    },
    "remoteUser": "appuser"
}